<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<interface>
    <object class="GtkWindow" id="one_rep_max_window">
        <property name="default_height">250</property>
        <property name="default_width">440</property>
        <property name="height_request">200</property>
        <property name="width_request">300</property>
        <child>
            <object class="GtkTable" id="table1">
                <property name="border_width">6</property>
                <property name="column_spacing">6</property>
                <property name="n_columns">3</property>
                <property name="n_rows">4</property>
                <property name="row_spacing">6</property>
                <property name="visible">True</property>
                <child>
                    <object class="GtkLabel" id="label1">
                        <property name="label" translatable="yes">Weight lifted:</property>
                        <property name="visible">True</property>
                        <property name="xalign">1.0</property>
                    </object>
                </child>
                <child>
                    <placeholder/>
                    <packing/>
                </child>
                <child>
                    <object class="GtkLabel" id="label2">
                        <property name="label" translatable="yes">Number of reps:</property>
                        <property name="visible">True</property>
                        <property name="xalign">1.0</property>
                    </object>
                    <packing>
                        <property name="bottom_attach">2</property>
                        <property name="top_attach">1</property>
                    </packing>
                </child>
                <child>
                    <object class="GtkHSeparator" id="hseparator1">
                        <property name="visible">True</property>
                    </object>
                    <packing>
                        <property name="bottom_attach">3</property>
                        <property name="right_attach">3</property>
                        <property name="top_attach">2</property>
                    </packing>
                </child>
                <child>
                    <object class="GtkLabel" id="label3">
                        <property name="label" translatable="yes">Predicted one-rep max:</property>
                        <property name="visible">True</property>
                        <property name="xalign">1.0</property>
                    </object>
                    <packing>
                        <property name="bottom_attach">4</property>
                        <property name="top_attach">3</property>
                    </packing>
                </child>
                <child>
                    <object class="ProxySpinButton" id="weight_spinbutton">
                        <property name="adjustment">0 1 10000 1 10 0</property>
                        <property name="data_type">float</property>
                        <property name="digits">2</property>
                        <property name="max_length">7</property>
                        <property name="model_attribute">weight_spinbutton</property>
                        <property name="numeric">True</property>
                        <property name="text" translatable="yes">0.00</property>
                        <property name="visible">True</property>
                        <property name="width_chars">8</property>
                    </object>
                    <packing>
                        <property name="left_attach">1</property>
                        <property name="right_attach">2</property>
                    </packing>
                </child>
                <child>
                    <object class="ProxySpinButton" id="reps_spinbutton">
                        <property name="adjustment">1 1 15 1 10 0</property>
                        <property name="data_type">int</property>
                        <property name="is_focus">True</property>
                        <property name="max_length">2</property>
                        <property name="model_attribute">reps_spinbutton</property>
                        <property name="numeric">True</property>
                        <property name="text" translatable="yes">1</property>
                        <property name="visible">True</property>
                    </object>
                    <packing>
                        <property name="bottom_attach">2</property>
                        <property name="left_attach">1</property>
                        <property name="right_attach">2</property>
                        <property name="top_attach">1</property>
                    </packing>
                </child>
                <child>
                    <object class="ProxyEntry" id="result_entry">
                        <property name="data_type">float</property>
                        <property name="editable">False</property>
                        <property name="model_attribute">result_entry</property>
                        <property name="visible">True</property>
                        <property name="width_chars">8</property>
                        <property name="xalign">1.0</property>
                    </object>
                    <packing>
                        <property name="bottom_attach">4</property>
                        <property name="left_attach">1</property>
                        <property name="right_attach">2</property>
                        <property name="top_attach">3</property>
                    </packing>
                </child>
                <child>
                    <object class="ProxyComboBox" id="unit_combobox">
                        <property name="data_type">int</property>
                        <property name="model_attribute"></property>
                        <property name="visible">True</property>
                    </object>
                    <packing>
                        <property name="left_attach">2</property>
                        <property name="right_attach">3</property>
                    </packing>
                </child>
                <child>
                    <placeholder/>
                    <packing>
                        <property name="left_attach">2</property>
                        <property name="right_attach">3</property>
                        <property name="top_attach">1</property>
                        <property name="bottom_attach">2</property>
                    </packing>
                </child>
                <child>
                    <object class="ProxyLabel" id="result_label">
                        <property name="data_type">unicode</property>
                        <property name="model_attribute">result_label</property>
                        <property name="visible">True</property>
                        <property name="xalign">0.0</property>
                    </object>
                    <packing>
                        <property name="bottom_attach">4</property>
                        <property name="left_attach">2</property>
                        <property name="right_attach">3</property>
                        <property name="top_attach">3</property>
                    </packing>
                </child>
            </object>
        </child>
    </object>
</interface>
