<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/bbcalc/measurement_system</key>
      <applyto>/apps/bbcalc/measurement_system</applyto>
      <owner>bbcalc</owner>
      <type>string</type>
      <default>Metric</default>
      <locale name="C">
        <short>Default System of Measurements</short>
        <long>Default System of Measurements: Metric, Imperial</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/bbcalc/default_gender</key>
      <applyto>/apps/bbcalc/default_gender</applyto>
      <owner>bbcalc</owner>
      <type>string</type>
      <default>Male</default>
      <locale name="C">
        <short>Default Gender</short>
        <long>Default Gender: Male, Female</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>